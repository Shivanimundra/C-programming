#include <stdio.h>
#include <stdlib.h>
int n;
int main()
{
    scanf("%d",&n);
    int a = sizeof(n)*8;
      n>>=a-1;
            
    if((n&1)==0)
    {
        printf("number is positive");
    }
    else
    {
        printf("number is negative");
    }
    return 0;
}
