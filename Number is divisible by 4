#include <stdio.h>
int n;
int main()
{
    scanf("%d",&n);
    while(n>0)
    {
      if(n==0)
      {
        printf("no.is divisibe by 4");
        break;
      }
       n=n-4; 
       
    }
    if(n<0)
    {
        printf("no.is not divisibe by 4");
    }
    

    return 0;
}
