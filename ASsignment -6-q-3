#include <stdio.h> 
char str[]="p2ro@gram84iZ./";
char str1[10];
int x=0;
int main() 
{ 
   for(int i=0;i<16;i++)
   {
       if(((64<str[i])&&(str[i]<91))||((96<str[i])&&(str[i]<122)))
       {
         str1[x]=str[i];  
          x++;
       }
   }
   for(int j=0;j<x;j++)
   {
     str[j]=str1[j]; 
     printf("%c",str[j]);
   }
     return 0; 
} 